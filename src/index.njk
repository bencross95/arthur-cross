    <div class="arthur3d-background">
        <model-viewer class="arthur3d-model" rotation-per-second="-2deg" camera-orbit="10deg 55deg 2m" auto-rotate id="model-b"  alt="" src="/assets/images/face.glb" shadow-intensity="1"></model-viewer>
    </div>


<div class="sectionMargin pb4">
    {% for project in collections.project | reverse %}

    <div class="flex justify-between home-project-div mt3">

        <div class="parent projectItemList">
            <div class="div1 listTitle">{{ project.data.title }}</div>
            <div class="div2 listTitle">{{ project.data.tag }}</div>
            <div class="div3 listTitle">{{ project.data.date | postDate }}</div>
            <div class="div4 mw7">{{ project.data.description }}</div>
        </div>
        <div class="home-grid-end flex justify-between">
            <div class="project-list-img"><img src="{{ project.data.image }}" alt="" height=auto width=auto></div>
            <div class=""><button onclick="window.location.href='{{ project.url }}';"">Open</button></div>
        </div>

    </div>

    {% endfor %}
</div>
