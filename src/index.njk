    <div class="arthur3d-div">
        <model-viewer class="arthur3d-model" rotation-per-second="2deg" camera-orbit="100deg 55deg 2m" auto-rotate id="model-f"  alt="" src="/assets/images/arthur3d.glb" shadow-intensity="1"></model-viewer>
