<div class="sectionMargin pb4">
    {% for project in collections.project | reverse %}
    <div class="parent pt4 projectItemList">
        <div class="div1 listTitle">{{ project.data.title }}</div>
        <div class="div2 listTitle">METAL Magazine issue 48</div>
        <div class="div3 listTitle">{{ project.data.date | postDate }}</div>
        <div class="div4"><img src="{{ project.data.image }}" alt="" height=auto width=100></div>
        <div class="div5"><button onclick="window.location.href='{{ project.url }}';"">Open</button></div>
        <div class="div6 mw7">{{ project.data.description }}</div>
    </div>
    {% endfor %}
</div>